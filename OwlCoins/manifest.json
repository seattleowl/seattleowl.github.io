{
    "name": "OwlCoins",
    "version": "0.1",
    "manifest_version" : 2,
    "description": "Digital currency for Scratch projects",
    "browser_action": {
        "default_icon": "logo.png"
    },
    "background" : {
        "scripts" : ["events.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://scratch.mit.edu/projects/*"],
            "js": ["varHide.js"]
        }
    ],
    "permissions": ["activeTab"]
}